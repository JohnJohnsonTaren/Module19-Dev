<?xml version="1.0"?>
<!DOCTYPE suppressions PUBLIC
    "-//Checkstyle//DTD SuppressionFilter Configuration 1.3//EN"
    "dtds/suppressions_1_2.dtd">

<suppressions>
    <!-- Приклади виключень -->
    <suppress files=".*[\\/]generated[\\/].*\.java" checks=".*"/>
    <suppress files=".*[\\/]Application\.java" checks="HideUtilityClassConstructor"/>
    <suppress files=".*[\\/]config[\\/].*\.java" checks=".*"/>
    
    <!-- Виключення для тестових файлів -->
    <suppress files=".*[\\/]src[\\/]test[\\/].*" checks="JavadocMethod"/>
    <suppress files=".*[\\/]src[\\/]test[\\/].*" checks="JavadocType"/>
</suppressions>